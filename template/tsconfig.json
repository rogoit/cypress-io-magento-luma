{
  "compilerOptions": {
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@/support/*": ["template/cypress/support/*"],
      "@/pages/*": ["template/cypress/pages/*"]
    },
    "module": "amd",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "ES2019",
    "lib": ["esnext", "dom"],
    "types": ["cypress", "node"],
    "allowSyntheticDefaultImports": true,
    "typeRoots": ["node_modules/@types"]
  },
  "include": ["**/*.ts"]
}
